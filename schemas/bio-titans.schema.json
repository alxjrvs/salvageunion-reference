{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "$id": "https://salvageunion.com/schemas/bio-titans.schema.json",
  "title": "Salvage Union Bio-Titans",
  "description": "Massive bio-engineered titan creatures in Salvage Union",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["structurePoints", "actions"],
    "allOf": [
      {
        "$ref": "shared/objects.schema.json#/definitions/entry"
      }
    ],
    "properties": {
      "structurePoints": {
        "$ref": "shared/common.schema.json#/definitions/positiveInteger"
      },
      "actions": {
        "type": "array",
        "items": {
          "$ref": "shared/objects.schema.json#/definitions/action"
        }
      },
      "traits": {
        "$ref": "shared/objects.schema.json#/definitions/traits"
      }
    }
  }
}
