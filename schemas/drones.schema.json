{
  "$schema": "https://json-schema.org/draft/2019-09/schema#",
  "$id": "https://salvageunion.com/schemas/drones.schema.json",
  "title": "drones",
  "description": "Autonomous drones in Salvage Union",
  "type": "array",
  "items": {
    "type": "object",
    "required": [
      "structurePoints",
      "techLevel",
      "salvageValue",
      "systems"
    ],
    "allOf": [
      {
        "$ref": "shared/objects.schema.json#/definitions/baseEntry"
      }
    ],
    "properties": {
      "structurePoints": {
        "$ref": "shared/common.schema.json#/definitions/positiveInteger"
      },
      "techLevel": {
        "$ref": "shared/common.schema.json#/definitions/techLevel"
      },
      "salvageValue": {
        "$ref": "shared/common.schema.json#/definitions/salvageValue"
      },
      "systems": {
        "$ref": "shared/objects.schema.json#/definitions/systems"
      }
    },
    "unevaluatedProperties": false
  }
}
